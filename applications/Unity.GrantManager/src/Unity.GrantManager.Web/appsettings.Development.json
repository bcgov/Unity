{
  "App": {
    "SelfUrl": "https://localhost:44342"
  },
  "ConnectionStrings": {
    "Default": "Host=localhost;port=5432;Database=UnityGrantManager;Integrated Security=True;Username=postgres;Password=admin",
    "Tenant": "Host=localhost;port=5432;Database=UnityGrantTenant;Integrated Security=True;Username=postgres;Password=admin"
  },
  "StringEncryption": {
    "DefaultPassPhrase": "g2IuZx7PwXDvCmlW"
  },
  "BackgroundJobs": {
    "IsJobExecutionEnabled": false,
    "Quartz": {
      "IsAutoRegisterEnabled": false
    },
    "IntakeResync": {      
      "Expression": "0 0/1 * * * ?"
    }
  },
  "Notifications": {
    "TeamsNotificationsWebhook": "",
    "ChesUrl": "https://ches-dev.api.gov.bc.ca/api/v1",
    "ChesTokenUrl": "https://dev.loginproxy.gov.bc.ca/auth/realms/comsvcauth/protocol/openid-connect/token",
    "ChesClientId": "",
    "ChesClientSecret": ""
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] ({Properties}) {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] ({Properties}) {Message:lj}{NewLine}{Exception}",
          "path": "logs/log.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "retainedFileCountLimit": 31,
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "IdentityProfileLogin": {
    "AutoCreateUser": true
  }
}
